generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id      Int      @id @default(autoincrement())
  clinic  String
  doctor  String
  time    DateTime
}
model Clinic {
  id        Int     @id
  name      String
  available Boolean
}
